{
  "name": "@sum.cumo/vue-datepicker",
  "version": "2.1.1",
  "description": "A simple Vue.js datepicker component. Supports disabling of dates, inline mode, translations",
  "license": "Apache-2.0",
  "homepage": "https://github.com/sumcumo/vue-datepicker",
  "repository": {
    "type": "git",
    "url": "git@github.com:sumcumo/vue-datepicker.git"
  },
  "author": "sum.cumo GmbH",
  "keywords": [
    "vue",
    "datepicker",
    "date-picker",
    "calendar",
    "typeable"
  ],
  "main": "dist/vuejs-datepicker.js",
  "module": "dist/vuejs-datepicker.esm.js",
  "unpkg": "dist/vuejs-datepicker.min.js",
  "files": [
    "src",
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:app && npm run build:locale",
    "build:app": "babel-node scripts/build.js",
    "build:locale": "babel-node scripts/build-locale.js",
    "clear": "rm -rf dist/*",
    "audit": "npm audit --registry=https://registry.npmjs.org",
    "serve": "vue-cli-service serve ./example/main.js",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "test": "jest --config test/unit/jest.conf.js --coverage",
    "release": "standard-version",
    "prerelease": "standard-version --dry-run",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "standard-version": {
    "releaseCommitMessageFormat": "chore(release): {{currentTag}}"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "7.10.4",
    "@babel/plugin-transform-runtime": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "babel-loader": "^8.1.0",
    "autoprefixer": "9.8.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "chalk": "4.1.0",
    "cleave.js": "1.6.0",
    "core-js": "3.6.5",
    "date-fns": "2.14.0",
    "eslint": "7.4.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-standard": "14.1.1",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-html": "6.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "^7.0.0-beta.3",
    "sass": "1.26.11",
    "sass-loader": "10.0.2",
    "semver": "7.3.2",
    "standard-version": "8.0.1",
    "vee-validate": "2.2.15",
    "vue": "^3.0.0",
    "vuepress": "1.5.2"
  }
}
